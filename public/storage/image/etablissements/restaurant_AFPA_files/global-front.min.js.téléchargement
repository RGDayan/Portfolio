changeImage(imageNum);};function changeImage(imageNum){if(opts.inprogress==false){opts.inprogress=true;opts.activeImage=imageNum;jQuery('#loading').show();jQuery('#lightboxImage').hide();jQuery('#hoverNav').hide();jQuery('#prevLink').hide();jQuery('#nextLink').hide();if(opts.slideNavBar){jQuery('#imageDataContainer').hide();jQuery('#imageData').hide();doChangeImage();}else{doChangeImage();}}};function doChangeImage(){imgPreloader=new Image();imgPreloader.onload=function(){var newWidth=imgPreloader.width;var newHeight=imgPreloader.height;if(opts.fitToScreen){var arrayPageSize=getPageSize();var maxWidth=arrayPageSize[2]-3*opts.borderSize;var maxHeight=arrayPageSize[3]-200;var ratio=1;if(newHeight>maxHeight){ratio=maxHeight/newHeight;}
