
jQuery(document).ready(function($){var goToTop=$('#goToTop');var bouge=function(){if($(this).scrollTop()!=0){goToTop.fadeIn();}else{goToTop.fadeOut();}};$(window).scroll(bouge);$(window).load(bouge);goToTop.click(function(){$('body,html').animate({scrollTop:0},800);});});var vvqflashvars={};var vvqparams={wmode:"opaque",allowfullscreen:"true",allowscriptaccess:"always"};var vvqattributes={};var vvqexpressinstall="http://4.cdnblog.fr/wp-content/mu-plugins/plugins/vipers-video-quicktags/resources/expressinstall.swf";